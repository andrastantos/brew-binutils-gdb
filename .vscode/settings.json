{
    "window.zoomLevel": -1,
    "search.exclude": {
        "**/*.dmp": true
    },
    "diffEditor.ignoreTrimWhitespace": false,
    "editor.renderWhitespace": "all",
"python.languageServer": "Pylance",
"security.workspace.trust.untrustedFiles": "open",
    "[c]": {
        "editor.tabSize": 2,
	"editor.insertSpaces": true,
    },
    "[python]": {
        "editor.tabSize": 4,
	"editor.insertSpaces": true,
    },
    "[exp]": {
        "editor.tabSize": 8,
	"editor.insertSpaces": true,
    },
    "cmake.configureOnOpen": false
}